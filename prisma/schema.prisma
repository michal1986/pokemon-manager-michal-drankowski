generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Pokemon {
  id      Int     @id @default(autoincrement())
  name    String
  type    String
  level   Int
  trainer String?
  height  Int     @default(0)
  weight  Int     @default(0)
  image   String  @default("")
}

